<compatibility-matrix version="5.0" type="framework">
    <!-- Required/optional HALs -->
    <hal format="hidl">
        <name>android.hardware.cas</name>
        <version>1.0</version>
    </hal>

    <hal format="hidl">
        <name>android.hardware.nfc</name>
        <version>1.2</version>
    </hal>

    <!-- Vendor optional HALs -->
    <hal format="hidl" optional="true">
        <name>vendor.nxp.nxpnfc</name>
        <version>2.0</version>
    </hal>

    <hal format="hidl" optional="true">
        <name>vendor.nxp.nxpnfclegacy</name>
        <version>1.0</version>
    </hal>

    <hal format="hidl" optional="true">
        <name>vendor.oppo.engnative.engineer</name>
        <version>1.0</version>
    </hal>

    <hal format="hidl" optional="true">
        <name>vendor.sprd.hardware.connmgr</name>
        <version>1.0</version>
    </hal>

    <hal format="aidl" optional="true">
        <name>vendor.sprd.hardware.enhance</name>
    </hal>

    <hal format="hidl" optional="true">
        <name>vendor.sprd.hardware.network</name>
        <version>1.0</version>
    </hal>

    <hal format="hidl" optional="true">
        <name>vendor.sprd.hardware.wcn</name>
        <version>1.0</version>
    </hal>

    <!-- System SDK levels -->
    <system-sdk>
        <version>28</version>
        <version>29</version>
        <version>30</version>
        <version>31</version>
        <version>32</version>
        <version>33</version>
    </system-sdk>
    <sepolicy>
        <kernel-sepolicy-version>30</kernel-sepolicy-version>
        <sepolicy-version>28.0</sepolicy-version>
        <sepolicy-version>29.0</sepolicy-version>
        <sepolicy-version>30.0</sepolicy-version>
        <sepolicy-version>31.0</sepolicy-version>
        <sepolicy-version>32.0</sepolicy-version>
        <sepolicy-version>33.0</sepolicy-version>
    </sepolicy>
    <avb>
        <vbmeta-version>1.0</vbmeta-version>
    </avb>
</compatibility-matrix>

